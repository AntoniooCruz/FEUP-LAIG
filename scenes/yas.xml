<yas>
    <scene root="room" axis_length="10.0"/>

    <views default="camera-perspective-0">
       <perspective id="camera-perspective-0" near="0.1" far="500" angle="45">
            <from x="15" y="7.0" z="17.0"/>
            <to x="0.0" y="1.0" z="0.0"/>
        </perspective>

    </views>

    <ambient>
        <ambient r="0.3" g="0.3" b="0.3" a="1.0"/>
        <background r="0.3" g="0.4" b="0.6" a="1.0"/>
    </ambient>

    <lights>
        <omni id="omni-light-0" enabled="1">
            <location x="0" y="0" z="0" w="1"/>
            <ambient r="0.5" g="0.5" b="0.5" a="1"/>
            <diffuse r="0" g="0" b="0" a="0"/>
            <specular r="0" g="0" b="0" a="0"/>
        </omni>

    </lights>

    <textures>
       
    </textures>

    <materials>
        <material id="material-1" shininess = "10.0" >
            <emission r="0.5" g="0.5" b="0" a="1" />
            <ambient r="0.2" g="0.2" b="0.2" a="1" />
            <diffuse r="0.5" g="0.5" b="0.5" a="1" />
            <specular r="0.5" g="0.5" b="0.5" a="1" />
        </material>

    </materials>

    <transformations>


    </transformations>


    <primitives>
        <primitive id="triangle">
            <triangle x1="1" y1="0" z1="0"
                      x2="0" y2="1" z2="0"
                      x3="0" y3="0" z3="2" />
        </primitive >
        <primitive id="rectangle">
          <rectangle x1="0" y1="0"
                     x2="1" y2="1" />
        </primitive >
        <primitive id="sphere">
          <sphere radius="1" slices="30"
                    stacks="10" />
        </primitive >
        <primitive id="cylinder">
          <cylinder base="0.3" top="0.3"
                    height="2" slices="30" stacks="10" />
        </primitive >

         <primitive id="torus">
          <torus inner="0.1" outer="2"
                 slices="40" loops="30" />
        </primitive >

        <primitive id="patch">
          <patch npartsU="1" npartsV="1" npointsU="3" npointsV="2">
                <controlpoint xx="1.0" yy="1.0" zz="1.0" />
                <controlpoint xx="1.1" yy="1.1" zz="1.2" />
                <controlpoint xx="1.5" yy="1.5" zz="2.0" />
                <controlpoint xx="1.5" yy="1.9" zz="3.0" />
                <controlpoint xx="2.0" yy="3.0" zz="5.0" />
                <controlpoint xx="6.0" yy="6.0" zz="6.0" />
          </patch>
        </primitive >

    </primitives >

    <components>

        <component id = "room">
            <transformation>
            </transformation>

            <materials>
                <material id="material-1"/>
            </materials>

            <texture id="none" length_s="1.0" length_t="1.0"/>

            <children>
                 <primitiveref id="cylinder"/>
            </children>
        </component>

    </components>
</yas>
